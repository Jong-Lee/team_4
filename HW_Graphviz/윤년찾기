import calendar
checklist = {'0' : 'Monday', '1' : 'Tuesday', '2':'Wednesday', '3':'Thursday', '4':'Friday', '5':'Saturday', '6':'Sunday'}
for i in range(1950, 2018):
    if calendar.monthrange(i, 2)[1] == 29:
        check = calendar.weekday(i, 2, 29)
        print("%d is a leap year and 1st, Jan, %d is %s" % (i,i,checklist['%s' % check])) 
